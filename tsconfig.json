{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "node16",
      "target": "ESNext",
      "noEmit": true,
      "sourceMap": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
